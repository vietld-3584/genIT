{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run --project unit",
    "test:api": "cross-env NODE_ENV=test vitest run --project api",
    "db:setup": "./scripts/setup-db.sh",
    "db:migrate:create": "mikro-orm migration:create",
    "db:migrate:up": "mikro-orm migration:up",
    "db:migrate:down": "mikro-orm migration:down",
    "db:migrate:list": "mikro-orm migration:list",
    "db:seed": "tsx lib/db/seed/seed.ts",
    "db:reset": "tsx lib/db/scripts/dev-db-reset.ts",
    "db:debug": "mikro-orm debug",
    "db:test": "tsx scripts/test-db-layer.ts"
  },
  "dependencies": {
    "@mikro-orm/cli": "^6.5.3",
    "@mikro-orm/core": "^6.5.3",
    "@mikro-orm/migrations": "^6.5.3",
    "@mikro-orm/postgresql": "^6.5.3",
    "axios": "^1.12.2",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "next": "15.5.3",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "reflect-metadata": "^0.2.2",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jsdom": "^27.0.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
